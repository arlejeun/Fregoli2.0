.cx-widget
	    z-index: 999999!important;
}

.cx-sidebar {
    /* top: 60px!important; */
    border:0!important;  /* to remove border line shown when the sidebar is hidden */
}    

.ChatUI .form .control-label {
    vertical-align: baseline!important;
}
    
/* color customization            */
.preset-blue .cx-img-map, .cx-img-map.preset-blue {
    background-image: url(../images/cx-widget.png)!important;
}

.ark .form-control {
    border: 1px solid #a9a9a9!important;
}

.cx-common-container {
	font-family: inherit!important;
}    

.ark td, .ark th {
    padding: 4px 0!important;
}

.cursor-not-allowed {
    cursor: not-allowed!important;
}

.cxw-hide {
    display: none!important;
}

/* Chat line breaks */
.ChatUI .transcript .message, .ChatUI .transcript > div .message {
    white-space: pre-wrap!important;
}

/* Chat input area hight increase*/
#genesys_webchat * textarea.input {
   /* height: 50px!important; */
}

 .ark.cx-common-container.cx-webchat .cx-titlebar {
    height: 75px!important;
 }

 .ChatUI .form .control-label {
        margin-right: 5px;
}

.cx-send-message-button, .cx-webchat-chat-button {
	color: #333333! important;
	background-color: #FDFDFD! important;
	box-shadow: 0px 0px 21px 2px rgba(0, 0, 0, 0.2);
}
 
