/* T H E M E S */

/* RED theme */
.cx-widget.cx-theme-red .cx-svg-icon-tone1
{
    fill: red;
}
.cx-widget.cx-theme-red .cx-btn-primary {
    background: red;    
    color: white;
}
.cx-widget.cx-theme-red .cx-svg-icon-tone2{
    fill: crimson;
}
.cx-widget.cx-theme-red, 
.cx-widget.cx-theme-red .cx-knowledge-center-wrapper {
    color: #333333; background: #fdfdfd;
}
.cx-widget.cx-theme-red .cx-knowledge-center-wrapper+.cx-button-group {
    color: #333333; background: #fdfdfd;
}
.cx-widget.cx-theme-red *, .cx-widget.cx-theme-red *:hover, .cx-widget.cx-theme-red *:focus{
